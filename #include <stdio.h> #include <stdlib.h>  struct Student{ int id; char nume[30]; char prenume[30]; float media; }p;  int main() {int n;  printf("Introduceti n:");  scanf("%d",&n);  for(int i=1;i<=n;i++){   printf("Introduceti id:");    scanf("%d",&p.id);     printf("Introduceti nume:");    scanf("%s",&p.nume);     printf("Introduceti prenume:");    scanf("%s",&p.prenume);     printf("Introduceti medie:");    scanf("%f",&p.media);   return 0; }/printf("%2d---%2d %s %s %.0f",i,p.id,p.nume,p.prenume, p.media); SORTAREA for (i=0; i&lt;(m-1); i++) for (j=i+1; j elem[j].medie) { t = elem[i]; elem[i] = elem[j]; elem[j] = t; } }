 return 0;
}

// printf("%2d---%2d %s %s %.0f",i,p.id,p.nume,p.prenume, p.media);
SORTAREA
for (i=0; i&lt;(m-1); i++)
for (j=i+1; j elem[j].medie)
{
t = elem[i];
